<Window x:Class="BillProject.DayDay"
       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BillProject"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600" d:DataContext="{d:DesignInstance local:DayDay}"
             Title="DayDay" Height="450" Width="650">
    <Window.Background>
        <ImageBrush ImageSource="/Resources/배경.PNG"/>
    </Window.Background>
    <Grid>
        <lvc:CartesianChart Series="{Binding SeriesCollctions}" LegendLocation="Left" Margin="10,31,71,10" ScrollBarFill="#FF634B4B">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="" Labels="{Binding Labels}" FontFamily="Segoe Script" FontSize="10" Foreground="#FF634B4B"></lvc:Axis>
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title=""  FontFamily="Segoe Script" FontSize="10" Foreground="#FF634B4B"></lvc:Axis>
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
    </Grid>
</Window>
